program pr;

const
  n = 10;

type
  arrt = array[1..n] of integer;

var
  Arr: arrt;
  i: integer;

procedure vice_versa(var A: arrt; i: integer);
begin
  for i := 1 to n do
    A[i] := A[i] + 42;
end;

procedure Print(A: arrt; i: integer);
begin
  for i := 1 to n do 
    Write(A[i], ' ');
end;

begin
  for i := 1 to n do
    read(arr[i]);
  Vice_versa(Arr, i);
  Print(Arr, i);
end.
